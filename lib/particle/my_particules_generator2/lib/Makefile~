SRC	=	load_2d_arr_from_file.c \
		open_file.c \
		mem_alloc_2d_array_int.c \
		mem_alloc_2d_array.c \
		my_atoi.c	\
		my_putstr.c

HEAD	=	my.h

CFLAGS	=	-W -Wall -Wextra -I ./

OBJ	=	$(SRC:.c=.o)

NAME	=	libmy.a

all: $(NAME)

$(NAME):	$(OBJ)
	ar rc $(NAME) $(OBJ)

clean:
	rm -rf $(OBJ)

fclean: clean
	rm -rf $(NAME)

re: fclean all

.PHONY: all clean fclean re
